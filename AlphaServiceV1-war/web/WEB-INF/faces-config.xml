        <navigation-case>
            <redirect/>
            <from-outcome>Home</from-outcome>
            <to-view-id>/Test.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <redirect/>
        <from-view-id>/Test.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>Login</from-outcome>
            <to-view-id>/UI/Login.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>
